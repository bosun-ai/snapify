{{ 'bootstrap3.4.1min.css' | asset_url | stylesheet_tag }}
{{ 'component-image-with-text.css' | asset_url | stylesheet_tag }}
{{ 'forsure-charts.css' | asset_url | stylesheet_tag }}


{% if customer.tags contains 'member' %}
