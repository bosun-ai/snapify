function fillReports(key) {
    // Unset the active key
    var elements = document.querySelectorAll('.title-text-reports');
    elements.forEach(function(element) {
        if (element.classList.contains('active')) {
            element.classList.remove('active');
        }
    });
    document.getElementById(key).classList.add('active');

    var reportEntry = document.getElementById('reports-entry');
    reportEntry.innerHTML = '';

    // Define a broader list of assignees for diversity
    var assignees = ['Tom', 'Martin', 'Alice', 'Sophia', 'Liam', 'Olivia', 'Noah', 'Emma', 'Ava', 'Isabella'];

    // Create a new table element
    var table = document.createElement('table');
    table.id = 'reports-table';
    table.style.width = '100%';

    // Create the table header
    var thead = document.createElement('thead');
    thead.innerHTML = `
        <tr>
            <th class="theader" style="width: 40%">Company</th>
            <th class="theader" style="width: 25%">Assignee</th>
            <th class="theader">Actions</th>
        </tr>`;
    table.appendChild(thead);

    // Create the table body
    var tbody = document.createElement('tbody');

    reports[key].forEach(function(report) {
        // Create a new table row
        var tr = document.createElement('tr');

        // Pick a random assignee from the list
        var assignee = assignees[Math.floor(Math.random() * assignees.length)];

        // Create the Company cell
        var tdCompany = document.createElement('td');
        tdCompany.classList.add('tentry');
        tdCompany.innerHTML = `<div class="hover-effect report-name" onclick="window.location.href='https://for-sure.net/pages/forsure-statistics';">${report.name}</div>`;
        tr.appendChild(tdCompany);

        // Create the Assignee cell
        var tdAssignee = document.createElement('td');
        tdAssignee.className = 'assignee';
        tdAssignee.classList.add('tentry');
        tdAssignee.style.textAlign = 'left'; // Aligns assignee names to the right
        tdAssignee.textContent = assignee;
        tr.appendChild(tdAssignee);

        // Create the Actions cell
        var tdActions = document.createElement('td');
        tdActions.classList.add('tentry');
        tdActions.style.marginLeft = 'auto'; // Aligns assignee names to the right
        if (key === 'done') {
            var svg = `<svg style="width: 20px;margin-right:1rem;cursor: pointer;" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><title>${report.info}</title><path fill="#40d48b" d="M438.6 105.4c12.5 12.5 12.5 32.8 0 45.3l-256 256c-12.5 12.5-32.8 12.5-45.3 0l-128-128c-12.5-12.5-12.5-32.8 0-45.3s32.8-12.5 45.3 0L160 338.7 393.4 105.4c12.5-12.5 32.8-12.5 45.3 0z"/></svg>`;
            tdActions.innerHTML = svg;
        } else if (key === 'to_send') {
            var imgOpen = '<img class="iconreport" width="20px" style="cursor: pointer;" src="{{ section.settings.open | image_url }}" alt="{{ section.settings.open.alt | escape }}" loading="lazy">';
            var svgAnalyse = '<svg class="iconreport" width="20px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352a144 144 0 1 0 0-288 144 144 0 1 0 0 288z"/></svg>';
            var imgSend = '<img class="iconreport" width="20px" style="cursor: pointer;margin-right: 0;" src="{{ section.settings.send_report | image_url }}" alt="{{ section.settings.send_report.alt | escape }}" loading="lazy">';
            var imgShare = '<img class="iconreport" width="20px" style="cursor: pointer;" src="{{ section.settings.share | image_url }}" alt="{{ section.settings.share.alt | escape }}" loading="lazy">';
            var imgDownload = '<img class="iconreport" width="20px" style="cursor: pointer;" src="{{ section.settings.download | image_url }}" alt="{{ section.settings.download.alt | escape }}" loading="lazy">';
            tdActions.innerHTML = `<div style="width: fit-content;margin: auto;">${svgAnalyse}${imgDownload}${imgShare}${imgOpen}${imgSend}</div>`;
