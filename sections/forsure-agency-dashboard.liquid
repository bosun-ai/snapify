    function populateCompaniesSelect() {
        // Get the select element by its ID
        var selectElement = document.getElementById('companies');
    
        // Iterate over each category in the reports object
        for (var category in reports) {
            if (reports.hasOwnProperty(category)) {
                // Get the array of companies in this category
                var companies = reports[category];
    
                // Iterate over each company in the array
                companies.forEach(function(company) {
                    // Create a new option element
                    var option = document.createElement('option');
                    option.value = company.name;  // Set the value attribute to the company name
                    option.text = company.name;   // Set the displayed text to the company name
                    
                    // Append the option to the select element
                    selectElement.appendChild(option);
                });
            }
        }
    
        // Refresh the select picker to update the UI (if using Bootstrap selectpicker)
        if (typeof $('.selectpicker').selectpicker === 'function') {
            $('.selectpicker').selectpicker('refresh');
        }
    }
    
