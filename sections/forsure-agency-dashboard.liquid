    var deadlinesEntry = document.getElementById('deadlines-entry');

    // Start building the table HTML
    var tableHtml = `<ul style="list-style: none; padding: 1rem 1rem 0 1rem; text-align: left;">`;

    // Iterate over each deadline in the deadlines array
    deadlines.forEach(function(deadline) {
        // Format the deadline date from "YYYY-MM-DD" to "DD.MM.YYYY"
        var dateParts = deadline.Deadline.split('-');
        var catImg = deadline.category;
        
        // Determine which SVG to use based on the category
        if (deadline.category == 'WEEE') {
            catImg = getWEEESVG();
        } else if (deadline.category == 'Batteries') {
            catImg = getBatteriesSVG();
        } else if (deadline.category == 'Packaging') {
            catImg = getPackagingSVG();
        }

        var formattedDate = `${dateParts[2]}.${dateParts[1]}.${dateParts[0]}`;

        // Create list item with structured layout
        tableHtml += `
            <li id="${deadline.country}-${deadline.category}-${deadline.Deadline}" class="deadline" onclick="selectDeadline(this)" style="padding: 0.5rem 0;">
                <div style="display: flex; align-items: center;">
                    <div style="flex: 1; font-size: large;font-weight: bold; text-align: left;margin-left:3rem">${deadline.country}</div>
                    <div style="flex: 1; display: flex; justify-content: center;margin-right: 3rem">${catImg}</div>
                    <div style="flex: 1; text-align: right; font-size: large;font-weight: bold;margin-right: 3rem;">${formattedDate}</div>
                </div>
            </li>`;
    });

    tableHtml += '</ul>';
    // Set the inner HTML of the div to the table HTML
    deadlinesEntry.innerHTML = tableHtml;
}


