{% schema %}
    {
      "name": "t:sections.forsure_agency_dashboard.name", 
      "class": "section",
      "settings": [
        {
            "type": "image_picker",
            "id": "send_report",
            "label": "Send Report"
          },
          {
            "type": "image_picker",
            "id": "open",
            "label": "Open"
          },
          {
            "type": "image_picker",
            "id": "info",
            "label": "Info"
          },
          {
            "type": "image_picker",
            "id": "share",
            "label": "Share"
          },
          {
            "type": "image_picker",
            "id": "download",
            "label": "Download"
          },
            {
                "type": "range",
                "id": "padding_top",
                "min": 0,
                "max": 100,
                "step": 4,
                "unit": "px",
                "label": "t:sections.all.padding.padding_top",
                "default": 36
              }, {
                "type": "range",
                "id": "padding_bottom",
                "min": 0,
                "max": 100,
                "step": 4,
                "unit": "px",
                "label": "t:sections.all.padding.padding_bottom",
                "default": 36
              }
      ],
      "presets": [
        {
          "name": "t:sections.forsure_agency_dashboard.presets.name"
        }
      ]
    }
    {% endschema %}
    {{ 'bootstrap3.4.1min.css' | asset_url | stylesheet_tag }}
    {% if customer.tags contains 'member' %}
      <style>
         .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}
        px;
        padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}
        px;
      }
    
    
      @media screen and (min-width: 750px) {
        .section-{{ section.id }}-padding {
          padding-top: {{ section.settings.padding_top }}
          px;
          padding-bottom: {{ section.settings.padding_bottom }}
          px;
        }
      }
        .grid-container {
          display: grid;
          grid-template-columns: 0.8fr 1fr 1fr 1fr; /* Four columns with varying sizes */
          grid-template-rows: repeat(6, 1fr); /* Three rows */
          gap: 10px; /* Space between grid items */
          grid-auto-flow: dense; /* Allows items to fill smaller gaps in the grid */
          height: 80vh;
        }
    
        .grid-item {
          border: 1px solid #ccc; /* Border for grid items */
          margin: 10px; /* Padding inside grid items */
          /* display: flex; */
          justify-content: center;
          align-items: center;
          text-align: center;
          border: 0.2rem solid #17161c;
          border-radius: 1rem;
          position: relative;
          /* overflow-x: auto; */
          overflow-x: auto;
          scrollbar-width: none;
          scrollbar-color: #2255ff #fafafa;
        }
        .grid-title {
          background-color: #484B52;
          color: #fafafa;
          font-weight: bold;
          width: 100%;
          /* position: absolute; */
          padding: 1rem;
          /* top: 0; */
          border-bottom: 0.2rem solid #17161c;
          text-align: left;
            position: sticky; /* Makes the title sticky */
            top: 0; /* Sticks the title at the top of the container */
            z-index: 1;
        }
        .deadlines {
          grid-column: 1 / 2; /* Span across one column */
          grid-row: 1 / span 5; /* Span across two rows */
        }
    
        .tasks {
          grid-column: 2 / 3; /* Span across two columns */
          grid-row: 1 / span 2; /* Span across one row */
        }
    
        .metrics {
          grid-column: 3 / 5; /* Fourth column */
          grid-row: 1 / span 3; /* First row */
        }
    
        .not-registered {
          grid-column: 2 / 3; /* Second column */
          grid-row: 3 / span 3; /* Second row */
        }
    
        .audits {
          grid-column: 3 / 4; /* Third column */
          grid-row: 4 / span 2; /* Second row */
        }
    
        .errors {
          grid-column: 4 / 5; /* Fourth column */
          grid-row: 4 / span 2; /* Second row */
        }
        #reports-entry {
            text-align: left;
        }
        .title-text-reports {
            padding: 0 1rem;
            cursor: pointer;
            font-weight: bold;
            width: 100%;
        }
        .active-tab {
            font-weight: bold; /* Highlight the active tab */
            border-bottom: 2px solid #17161c; /* Add a bottom border to the active tab */
        }
        .active {
            background-color: #2255ff44;
        }
        .deadline {
            margin-bottom: 1rem;
            border-radius: 1rem;
            border: 0.2rem solid #17161c;
            padding: 1rem 1rem 1rem 1rem;
            cursor: pointer;
        }
        .activeDeadline {
            background: #2255ff;
            color: #fafafa;
        }
        .glyphicon-ok:before {
            content: "\2713"
        }
        .filter-group {
            margin: 0 2rem;
        }
        .scroll-entry {
            padding: 1rem;
            text-align: left;
            overflow-y: auto; /* Allows scrolling for the content */
            height: 100%;
        }
        .tab {
            overflow: hidden;
            border-bottom: 1px solid #ccc;
            display: flex;
            width: 25%;
        }
    
        /* Style the buttons inside the tab */
        .tab button {
            background-color: inherit;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 14px 16px;
            transition: 0.3s;
            font-size: 17px;
        }
    
        /* Change background color of buttons on hover */
        .tab button:hover {
            background-color: #ddd;
        }
    
        /* Create an active/current tablink class */
        .tab button.active {
            background-color: #ccc;
        }
    
        /* Style the tab content */
        .tabcontent {
            display: none;
            padding: 16px 12px;
            border-top: none;
        }
        .indented-text {
        display: inline-block;
        margin-left: 20px; /* Adjust the value as needed to control the indentation */
    }
    
    .hover-effect {
        text-decoration: none; /* No underline by default */
        font-weight: bold;
        transition: color 0.3s ease, text-decoration 0.3s ease; /* Smooth transition for color and underline */
    }
    
    .hover-effect:hover {
        color: #2255ff; /* Change to your desired hover color */
        text-decoration: underline; /* Underline the text on hover */
    }
    .theader {
        border: 0.2rem solid #17161c;
    }
    .tentry {
        border: none!important;
        padding: 1rem!important;
        text-align: left;
    }
    .icon {
        margin-right: 1rem;
    }
    .activeDeadline div div svg #Electronics .st2 {
        stroke: #fafafa;
    }
    .activeDeadline div div svg #Electronics .st5 {
        stroke: #fafafa;
        fill: #fafafa;
    }
    .activeDeadline div div svg #Batterie .st2  {
        stroke: #fafafa;
        /* fill: #fafafa; */
    }
    .activeDeadline div div svg #Batterie .st5  {
        stroke: #fafafa;
        fill: #fafafa;
    }

    .activeDeadline div div svg #Packaging .st2  {
        stroke: #fafafa;
        /* fill: #fafafa; */
    }
    .activeDeadline div div svg #Packaging .st5  {
        stroke: #fafafa;
        /* fill: #fafafa; */
    }
    .iconreport {
        margin-right: 3rem;
        cursor: pointer;
    }
    .report-entry {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 0.5rem;
}

.report-content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
}

.report-name {
    flex: 1;
    cursor: pointer;
}

.assignee {
    flex: 0;
    margin-left: 1rem;
    text-align: left;
    white-space: nowrap;
    margin-right: 15rem;
}
  .dropdown-menu .active a, .dropdown-menu .active a:focus, .dropdown-menu .active a:hover {
    background-color: #2255ff
  }
