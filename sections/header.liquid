{{ 'section-header.css' | asset_url | stylesheet_tag }}
{%- style -%}
  .header__heading-logo {
    max-width: {{ section.settings.logo_width }}px;
  }
  .header {
    padding-top: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.5 | round: 0 }}px;
  }

  .section-header {
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

  @media screen and (min-width: 990px) {
    .header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  @media screen and (min-width: 1200px) {
    .fixed-width-img {
      width: 220px;
    }
  }


  /* Container for the blocks */
  .block-container {
      display: flex;
      position: absolute;
      top: calc(50vh + 6rem);
      left: 50%;
      transform: translateX(-50%) translateY(-50%);
      justify-content: center;
      align-items: center;
      gap: 100px;
      padding: 50px;
      z-index: 3; 
  }
  
  /* Styling for each block */
  .block {
      width: 400px;
      padding: 3rem;
      background-color: #fafafa;
      border-radius: 2rem;
      /* border: 0.2rem solid #17161c; */
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      font-family: Arial, sans-serif;
      text-align: center;
      text-decoration: none;
  }

  .block svg {
      width: 60%;
  }
  .block h2 {
    margin: 0;
    margin-top: 1rem !important;
  }

  /* Add hover effect */
  .block:hover {
      background-color: #e0e0e0;
      cursor: pointer;
  }

  @media screen and (max-width: 1000px) {
    .block {
      width: 40vw;
    }
    .block-container {
      gap: 10vw;
    }
  }

  @media screen and (max-width: 450px) {
    .block {
      padding: 1rem;
    }
  }
  @media screen and (max-width: 800px) {
    .block {
      width: 50vw;
    }
    .block-container {
      flex-direction: column;
      top: 13rem;
      transform: translateX(-50%);
    }
  }
  #grey-overlay {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background-color: rgba(0, 0, 0, 0.5); /* Black with 50% opacity */
    z-index: 2; /* Displayed behind the modal */
  }
  @media (max-width: 550px) {
  .contactText br {
    display: inline; /* Show the line break on small screens */
  }
}

@media (min-width: 551px) {
  .contactText br {
    display: none; /* Hide the line break on larger screens */
  }
}
{%- endstyle -%}

