                                  {% render 'icon-arrow' %}
                                  {{ section.settings.back_button | escape }}
                                </button>
                                <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                  <a href="{{ link.url }}" hreflang="{{ link.title | escape | downcase }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                          {{ link.title | escape }}
                                        </a>
                                  {%- for childlink in link.links -%}
