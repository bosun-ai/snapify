       {% endif %}
       <script>
            const blockContainer = document.getElementById('block-container');
            const greyOverlay = document.getElementById('grey-overlay');

            function handleClickOutside(event, toggleButton) {
              console.log('Listening');
              if (blockContainer.style.display === 'flex' && !blockContainer.contains(event.target) && event.target !== toggleButton) {
                hideContactOptions(toggleButton);
              }
            }

            function showContactOptions() {
              console.log('Displaying');
              blockContainer.style.display = 'flex';
              greyOverlay.style.display = 'block'; // Show the grey background
              const toggleButton = document.getElementById('disappearbutt');
              // Adding named function to the event listener
              document.addEventListener('click', handleClick);
            }

            function showContactOptionsMobile() {
              console.log('Displaying');
              blockContainer.style.display = 'flex';
              greyOverlay.style.display = 'block'; // Show the grey background
              const toggleButton = document.getElementById('disappearbuttMobile');
              // Adding named function to the event listener
              document.addEventListener('click', handleClickMobile);
            }

            // Named function to handle the main toggle button
            function handleClick(event) {
              const toggleButton = document.getElementById('disappearbutt');
              handleClickOutside(event, toggleButton);
            }

            // Named function to handle the mobile toggle button
            function handleClickMobile(event) {
              const toggleButton = document.getElementById('disappearbuttMobile');
              handleClickOutside(event, toggleButton);
            }

            // Updated hideContactOptions to remove the correct event listener
            function hideContactOptions(toggleButton) {
              blockContainer.style.display = 'none';
              greyOverlay.style.display = 'none'; // Hide the grey background
              console.log(`removing event listener for ${toggleButton.id}`);

              // Check which toggleButton was passed and remove the correct event listener
              if (toggleButton.id === 'disappearbutt') {
                document.removeEventListener('click', handleClick);
              } else if (toggleButton.id === 'disappearbuttMobile') {
                document.removeEventListener('click', handleClickMobile);
              }
            }
      </script>
