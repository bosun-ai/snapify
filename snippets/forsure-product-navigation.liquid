}

#headerIcons #gear-icon svg{
    fill: #17161c;
    width: 28.5px;
    height: 28.5px;
}
#agency-home-icon svg {
  width: 26px;
}

.iconbig {
  justify-content: space-evenly;
}

.iconsmall {
  justify-content: space-between;
  margin-left: 0.5rem;
  margin-right: 0.5rem;
}

.iconsmall #notification-badge-header {
    top: 3px!important;
    right: -4px!important;
