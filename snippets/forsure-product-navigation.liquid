  <script>
    async function getNavbarElements() {
        const flashMessage = sessionStorage.getItem('flashMessage');
        if (flashMessage) {
          showMessage(flashMessage, true);
          sessionStorage.removeItem('flashMessage'); // clear so it only shows once
        }
        try {
            var data = await unifiedSendRequest(NAVBAR, {method: 'GET', headerArgs: {'Content-Type': 'application/json'}});
            window.userPermissions = data;
            var available_url = null;
            for (const [nav_element, show] of Object.entries(data)) {
                if (show === true) {
                  // Add 'greyed_out' class to elements that should be hidden or disabled
                  const element = document.getElementById(`a-${nav_element}`);
                  const wrapper = document.getElementById(`list-element-${nav_element}`);
                  if (element) {
                      element.classList.remove('greyed_out');
                      wrapper.classList.remove('greyed_out_wrapper');
                      if (available_url === null) {
                        available_url = nav_element;
                      }
                  }
                } else if (show === false) {
                } else if (nav_element === 'companies') {
                  selectedCompany = null;
                  if (Object.keys(data['companies']).length > 1) {
                    document.getElementById('headerIcons').classList.add('iconsmall');
                    document.getElementById('companiesWrapper').style.display = 'flex';
                    document.getElementById('companiesDropdown').innerHTML = '';
                    for (const [company, select] of Object.entries(data['companies'])) {
                      document.getElementById('companiesDropdown').innerHTML += `<option value="${company}">${company.replace('/', '')}</option>`;
                      if (select === true) {
                        selectedCompany = company;
                      }
                    }
                    if (selectedCompany !== null) {
                      document.getElementById('companiesDropdown').value = selectedCompany;
                    }
                    $('#companiesDropdown').selectpicker('refresh');
                  } else {
                    document.getElementById('companiesWrapper').style.display = 'none';
                    document.getElementById('headerIcons').classList.add('iconbig');
                    document.getElementById('guideButton').style.margin = 'auto 1rem';
                    document.getElementById('guideButton').style.marginTop = '11px';
                    document.getElementById('gear-icon').style.margin = 'auto 1rem';
                    document.getElementById('gear-icon').style.marginTop = '7px';
                    document.getElementById('notificationBell').style.margin = 'auto 1rem';
                    document.getElementById('notificationBell').style.marginTop = '1rem';
                    document.getElementById('ul-submenu').style.marginTop = '0rem';
                    document.getElementById('agency-home-icon').style.display = 'none';
                    document.getElementById('notification-badge-header').style.top = '0px';
                  }
                  
                } else {
                  // Special case for 'forsure-statistics' if no orders received
                  const tooltip = document.getElementById(`greyedout-tooltip-${nav_element}`);
                  tooltip.innerText = show;
                }
            }
            if (data['{{ currentLink }}'] != true && data['{{ currentLink }}?logged_in'] != true) {
                if (available_url === null) {
                  window.location.href = '/';
                } else {
                  if (`{{ currentPath }}` === '') {
                    window.location.href = `pages/${available_url}`;
                  } else {
                    window.location.href = `{{ currentPath }}/${available_url}`;
                  }
                }
            }
          if (window.userPermissions['forsure-agency-dashboard']) {
            document.getElementById('agency-home-icon').style.cursor = 'pointer';
            document.getElementById('agency-home-icon').title = 'Agency Overview';
          }
        } catch (error) {
            console.error('Failed to get navigation:', error);
            showMessage('Failed to get navigation: ' + error.message, false);
        }
    }
    function showAgencyDashboard() {
      if (window.userPermissions['forsure-agency-dashboard'] === true) {
        window.location.href="https://for-sure.net/pages/forsure-agency-dashboard";
      }
    }


    document.getElementById('companiesDropdown').addEventListener('change', async function() {
      console.log(document.getElementById('companiesDropdown').value);
      try {
        const formData = new FormData();
        formData.append('company_name', document.getElementById('companiesDropdown').value);
        var data = await unifiedSendRequest(ACCESS_SWITCH_COMPANY, {method: 'POST', formData: formData});
        company = document.getElementById('companiesDropdown').value.replace('/', '');
        const message = `Switched to company ${company}`;
        sessionStorage.setItem('flashMessage', message); // store in session storage
        window.location.reload();
        {% comment %} window.location.href = `{{ routes.root_url }}`; {% endcomment %}
      } catch (error) {
        console.error('Failed to get navigation:', error);
        showMessage('Failed to get navigation: ' + error.message, false);
      }
    });
    document.addEventListener('DOMContentLoaded', getNavbarElements);
  </script>
